          <table:table-cell table:style-name="Data_Cell"><text:p>10%</text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p></text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p></text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p></text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p></text:p></table:table-cell>
        </table:table-row>

        <!-- Category: Cost -->
        <table:table-row>
          <table:table-cell table:style-name="Category_Cell" table:number-columns-spanned="6">
            <text:p>COST</text:p>
          </table:table-cell>
        </table:table-row>

        <table:table-row>
          <table:table-cell table:style-name="Data_Cell"><text:p>License Cost (Annual)</text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p>15%</text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p></text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p></text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p></text:p></table:table-cell>
