          <table:table-cell table:style-name="Score_Good"><text:p>5</text:p></table:table-cell>
        </table:table-row>

        <table:table-row>
          <table:table-cell table:style-name="Data_Cell"><text:p>On-Premises Option</text:p></table:table-cell>
          <table:table-cell table:style-name="Data_Cell"><text:p>10%</text:p></table:table-cell>
