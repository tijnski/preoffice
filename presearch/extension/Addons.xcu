            <prop oor:name="Title" oor:type="xs:string">
              <value xml:lang="en">PreDrive Settings...</value>
            </prop>
            <prop oor:name="Target" oor:type="xs:string">
              <value>_self</value>
            </prop>
          </node>
        </node>
      </node>
    </node>

    <!-- Toolbar -->
    <node oor:name="OfficeToolBar">
      <node oor:name="com.presearch.preoffice.toolbar" oor:op="replace">
        <node oor:name="t01" oor:op="replace">
          <prop oor:name="URL" oor:type="xs:string">
            <value>service:com.presearch.prepanda.Ask</value>
          </prop>
          <prop oor:name="Title" oor:type="xs:string">
            <value xml:lang="en">Ask PrePanda</value>
          </prop>
          <prop oor:name="Target" oor:type="xs:string">
            <value>_self</value>
          </prop>
          <prop oor:name="Context" oor:type="xs:string">
            <value>com.sun.star.text.TextDocument,com.sun.star.sheet.SpreadsheetDocument,com.sun.star.presentation.PresentationDocument</value>
          </prop>
        </node>
        <node oor:name="t02" oor:op="replace">
          <prop oor:name="URL" oor:type="xs:string">
            <value>service:com.presearch.prepanda.Improve</value>
          </prop>
          <prop oor:name="Title" oor:type="xs:string">
            <value xml:lang="en">Improve</value>
          </prop>
          <prop oor:name="Target" oor:type="xs:string">
            <value>_self</value>
          </prop>
          <prop oor:name="Context" oor:type="xs:string">
            <value>com.sun.star.text.TextDocument,com.sun.star.sheet.SpreadsheetDocument,com.sun.star.presentation.PresentationDocument</value>
          </prop>
        </node>
      </node>
    </node>

    <!-- Context Menu (Right-click) -->
    <node oor:name="OfficeContextMenu">
      <node oor:name="Writer">
        <node oor:name="com.presearch.prepanda.context" oor:op="replace">
          <node oor:name="Submenu">
            <node oor:name="c01" oor:op="replace">
              <prop oor:name="URL" oor:type="xs:string">
                <value>service:com.presearch.prepanda.Summarize</value>
              </prop>
              <prop oor:name="Title" oor:type="xs:string">
                <value xml:lang="en">PrePanda: Summarize</value>
              </prop>
              <prop oor:name="Target" oor:type="xs:string">
                <value>_self</value>
              </prop>
            </node>
            <node oor:name="c02" oor:op="replace">
              <prop oor:name="URL" oor:type="xs:string">
                <value>service:com.presearch.prepanda.Improve</value>
              </prop>
              <prop oor:name="Title" oor:type="xs:string">
                <value xml:lang="en">PrePanda: Improve</value>
              </prop>
              <prop oor:name="Target" oor:type="xs:string">
                <value>_self</value>
              </prop>
            </node>
            <node oor:name="c03" oor:op="replace">
              <prop oor:name="URL" oor:type="xs:string">
                <value>service:com.presearch.prepanda.Translate</value>
              </prop>
              <prop oor:name="Title" oor:type="xs:string">
                <value xml:lang="en">PrePanda: Translate</value>
              </prop>
              <prop oor:name="Target" oor:type="xs:string">
                <value>_self</value>
              </prop>
            </node>
          </node>
        </node>
      </node>
    </node>

    <!-- Keyboard Shortcuts -->
    <node oor:name="OfficeAcceleratorKeyActions">
      <node oor:name="com.presearch.prepanda.shortcuts" oor:op="replace">
        <node oor:name="s01" oor:op="replace">
          <prop oor:name="URL" oor:type="xs:string">
            <value>service:com.presearch.prepanda.Ask</value>
          </prop>
          <prop oor:name="KeyCode" oor:type="xs:string">
            <value>P</value>
          </prop>
          <prop oor:name="Modifiers" oor:type="xs:string">
            <value>CTRL+SHIFT</value>
          </prop>
        </node>
      </node>
    </node>

